{
    "manifest_version": 2,
  
    "name": "pshow",
    "description": "pshow quotes",
    "version": "1.0",

    "web_accessible_resources": [
      "index.html",
      "pshow.js",
      "main.css",
      "pshownew.json"
    ],
    "icons": { "16": "pshowLogo16.png",
      "48": "pshowLogo48.png",
     "128": "pshowLogo128.png"
    },
    "permissions": [
      "contextMenus",
      "tabs",
      "http://*/*",
      "https://*/*",
      "<all_urls>"
    ], 
    "background": {
      "scripts": []
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }